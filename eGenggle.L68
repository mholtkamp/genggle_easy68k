00001394                          2205  ; _Ball_CheckPegCollisions
00001394                          2206  ;
00001394                          2207  ; Private subroutine that iterates through 
00001394                          2208  ; the peg list and checks if the ball is
00001394                          2209  ; overlapping any pegs. Handles collision 
00001394                          2210  ; when ball is overlapping a peg.
00001394                          2211  ;
00001394                          2212  ; Input:
00001394                          2213  ;   a0.l = pointer to ball struct 
00001394                          2214  ; ------------------------      
00001394                          2215  _Ball_CheckPegCollisions:
00001394                          2216      
00001394                          2217  REGS REG a0/a1/d0 
00001394                          2218  
00001394  43F8 0006               2219      lea Pegs, a1            ; get the peg array 
00001398  4280                    2220      clr.l d0                ; d0 = loop counter 
0000139A                          2221      
0000139A                          2222  .loop
0000139A                          2223      
0000139A  1229 0011               2224      move.b M_PEG_ACTIVE(a1), d1 
0000139E  4A01                    2225      tst.b d1
000013A0  6700 00C2               2226      beq .continue           ; continue if peg is inactive 
000013A4                          2227      
000013A4                          2228      ; Otherwise, peg is active. So check if the ball 
000013A4                          2229      ; is hitting it. 
000013A4                          2230      ; save regs 
000013A4  48E7 80C0               2231      movem.l REGS, -(sp) 
000013A8  4EB9 000015A2           2232      jsr Rect_OverlapsRect
000013AE  2200                    2233      move.l d0, d1           ; save result in d1 
000013B0                          2234      ; restore regs 
000013B0  4CDF 0301               2235      movem.l (sp)+, REGS
000013B4                          2236      
000013B4  4A81                    2237      tst.l d1 
000013B6  6700 00AC               2238      beq .continue 
000013BA                          2239      
000013BA                          2240      ; So after a bunch of testing and experimenting, I've decided to 
000013BA                          2241      ; use a very basic collision model where the ball bounces in the 
000013BA                          2242      ; exact same direct as the vector between the ball's position 
000013BA                          2243      ; and the peg's position. 
000013BA                          2244      
000013BA                          2245      ; Calculate the displacement vector (dispVec)
000013BA                          2246      ; The dispVec is the vector difference of the 
000013BA                          2247      ; positions of the ball and peg. The dispVec is 
000013BA                          2248      ; important because it shows what direction the ball 
000013BA                          2249      ; is from the peg.
000013BA  2228 0000               2250      move.l M_BALL_X(a0), d1 
000013BE  2428 0004               2251      move.l M_BALL_Y(a0), d2
000013C2  2629 0000               2252      move.l M_PEG_X(a1), d3 
000013C6  2829 0004               2253      move.l M_PEG_Y(a1), d4 
000013CA                          2254      
000013CA                          2255      ; subtract peg pos from ball pos 
000013CA  9283                    2256      sub.l d3, d1
000013CC  9484                    2257      sub.l d4, d2                ; <d1, d2> is the displacement vector now
000013CE                          2258      
000013CE  2628 0010               2259      move.l M_BALL_XVEL(a0), d3 
000013D2  2828 0014               2260      move.l M_BALL_YVEL(a0), d4  ; <d3, d4> is the velocity vector 
000013D6                          2261      
000013D6                          2262      ; Get the magnitude of the velocity 
000013D6  2A03                    2263      move.l d3, d5 
000013D8  C7C5                    2264      muls d5, d3     ; square the xvel
000013DA  E083                    2265      asr.l #8, d3    ; return to 24.8 format 
000013DC  2A04                    2266      move.l d4, d5 
000013DE  C9C5                    2267      muls d5, d4     ; square the yvel 
000013E0  E084                    2268      asr.l #8, d4    ; return to 24.8 format 
000013E2  D684                    2269      add.l d4, d3    ; add the squares of the x/y components 
000013E4                          2270      
000013E4                          2271      ; clamp the magnitude so it stays within sqrt table 
000013E4  E083                    2272      asr.l #8, d3             ; convert to int 
000013E6  0C83 00000080           2273      cmpi.l #MAX_SQRT_INPUT,d3 
000013EC  6500 0008               2274      blo .no_vel_clamp
000013F0  263C 00000080           2275      move.l #MAX_SQRT_INPUT, d3 
000013F6                          2276  .no_vel_clamp
000013F6  E383                    2277      asl.l #1, d3 
000013F8  47F8 117A               2278      lea SqrtTable, a3 
000013FC  D7C3                    2279      add.l d3, a3 
000013FE  4283                    2280      clr.l d3 
00001400  3613                    2281      move.w (a3), d3   ; d3 = magnitude of velocity 
00001402                          2282      
00001402                          2283      ; Dampen the velocity a tiny bit for some realistic collision 
00001402  283C 000000E0           2284      move.l #DAMPENING_COEFFICIENT, d4 
00001408  C7C4                    2285      muls d4, d3       
0000140A  E083                    2286      asr.l #8, d3        ; d3 = the new speed of the ball
0000140C                          2287      
0000140C                          2288      ; Normalize the displacement vector for performing projection 
0000140C                          2289      ; first step is to get magnitude squared 
0000140C  2A01                    2290      move.l d1, d5 
0000140E  CBC1                    2291      muls d1, d5
00001410  E085                    2292      asr.l #8, d5 
00001412  2C02                    2293      move.l d2, d6
00001414  CDC2                    2294      muls d2, d6 
00001416  E086                    2295      asr.l #8, d6
00001418  DA86                    2296      add.l d6, d5        ; d5 = magnitude squared 
0000141A                          2297      
0000141A                          2298      ; Next step to normalizing is to find the magnitude (sqrt of magnitude squared)
0000141A  E085                    2299      asr.l #8, d5        ; convert mag squared from 24.8 to int 
0000141C  0C85 00000080           2300      cmpi.l #MAX_SQRT_INPUT, d5 
00001422  6500 0008               2301      blo .skip_mag_clamp
00001426  2A3C 00000080           2302      move.l #MAX_SQRT_INPUT, d5  ; clamp if outside of table index range 
0000142C                          2303  .skip_mag_clamp
0000142C  E38D                    2304      lsl.l #1, d5                ; get the index into the sqrt table in bytes 
0000142E  47F8 117A               2305      lea SqrtTable, a3 
00001432  D7C5                    2306      add.l d5, a3 
00001434  4285                    2307      clr.l d5                    ; clear the long because we are about to store a positive word in this reg 
00001436  3A13                    2308      move.w (a3), d5             ; d5 = magnitude 
00001438                          2309      
00001438                          2310      ; Last step of normalizing is to divide dispVec by the magnitude 
00001438  E181                    2311      asl.l #8, d1        ; the divors can be in 16.16 format 
0000143A  E182                    2312      asl.l #8, d2 
0000143C  83C5                    2313      divs d5, d1 
0000143E  85C5                    2314      divs d5, d2 
00001440  48C1                    2315      ext.l d1            ; now d1 should be in 24.8
00001442  48C2                    2316      ext.l d2            ; and d2 should be in 24.8
00001444                          2317      
00001444                          2318      ; <d1, d2> = the normalized displacement vector 
00001444                          2319      ; Now (for basic collision) we can just multiply the dampened velocity 
00001444                          2320      ; with the normalized displacement vector and then set that as the new 
00001444                          2321      ; xvel/yvel of the ball. 
00001444  C3C3                    2322      muls d3, d1
00001446  E081                    2323      asr.l #8, d1 
00001448  C5C3                    2324      muls d3, d2
0000144A  E082                    2325      asr.l #8, d2 
0000144C                          2326      
0000144C  2141 0010               2327      move.l d1, M_BALL_XVEL(a0)
00001450  2142 0014               2328      move.l d2, M_BALL_YVEL(a0)
00001454                          2329      
00001454                          2330      ; Consume the peg! omnom (deactivate it and add to score)
00001454  48E7 80C0               2331      movem.l REGS, -(sp) 
00001458  2049                    2332      move.l a1, a0         ; put peg into a0 for param 
0000145A  4EB9 0000151C           2333      jsr Peg_Consume 
00001460  4CDF 0301               2334      movem.l (sp)+, REGS 
00001464                          2335      
00001464                          2336  .continue
00001464  D3FC 00000020           2337      adda.l #PEG_DATA_SIZE, a1 
0000146A  5280                    2338      addq.l #1, d0 
0000146C  B0B8 041E               2339      cmp.l LevelPegCount, d0 
00001470  6600 FF28               2340      bne .loop 
00001474                          2341      
00001474  4E75                    2342      rts 
